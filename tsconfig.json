{
  "compilerOptions": {
    "outDir": "./build/",
    "module": "es6",
    "target": "es6",
    "declaration": true /* Generates corresponding '.d.ts' file. */,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "build/**/*", "**/*.spec.ts"]
}
